@model IEnumerable<JobSystem.Mvc.ViewModels.Users.UserAccountListViewModel>

<table>
    <tr>
        <th>
            Name
        </th>
        <th>
            Email Address
        </th>
        <th>
            Job Title
        </th>
        <th>
        </th>
    </tr>
    @foreach (var item in Model)
 {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmailAddress)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JobTitle)
            </td>
            <td>
                <a href="#" class="editUserButton" id="@item.Id">Edit</a>
            </td>
        </tr>
 }
</table>
